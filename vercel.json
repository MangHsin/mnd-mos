{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build"
      },
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
        {
        "src": "/api/(.*)",
        "dest": "/api/$1"
        },
        {
        "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot))",
        "dest": "/$1"
        },
        {
        "src": "/(.*)",
        "dest": "/index.html"
        }
    ]
  }